{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "swagger/paths/**/*.yml",
                    "models/**/*.js",
                    "node_modules/swagger-ui-dist/**" 
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/api/docs/(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}